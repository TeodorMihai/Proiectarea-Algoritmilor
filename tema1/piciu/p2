#include <cstdio>
 
#define MAX_N 2125
 
int V, N, M;
char T[MAX_N][MAX_N];
int S[MAX_N], Mat[MAX_N][MAX_N];
 
inline int minim(const int &a, const int &b)
{
    return (a < b)? (a) : (b);
}
 
inline int min(const int &a, const int &b, const int &c)
{
    return minim(minim(a,b),c);
}
 
void citire()
{
    scanf("%d %d",&V, &N);
 
    while(V--)
        for(int i = 1; i <= N; ++i)
        {
            int x;
            scanf("%d",&x);
 
            T[x][i] = 1;
        }
 
    scanf("%d",&M);
    for(int i = 1; i <= M; ++i)
        scanf("%d",S+i);
}
 
void solve()
{
    for(int i = 1; i <= N; ++i)
        Mat[i][0] = i;
    for(int j = 1; j <= M; ++j)
        Mat[0][j] = j;
 
    for(int i = 1; i <= N; ++i)
        for(int j = 1; j <= M; ++j)
        {
            if(T[S[j]][i])
                Mat[i][j] = Mat[i-1][j-1];
            else
                Mat[i][j] = min(Mat[i-1][j-1] + 1, Mat[i-1][j] + 1, Mat[i][j-1] + 1);
 
        }
         
    printf("%d\n",Mat[N][M]);
}
 
int main()
{
    freopen("note.in","rt",stdin);
    freopen("note.out","wt",stdout);
 
    citire();
    solve();
}