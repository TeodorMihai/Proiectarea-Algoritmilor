include <stdio.h>
#include <string.h>
 
int n, sol, f[200];
char s[200];
 
int main ()
{
    freopen ("palind.in", "r", stdin);
    freopen ("palind.out", "w", stdout);
     
    int t, i, st, dr;
     
    scanf ("%d\n", &t);
    while (t --)
    {
        memset (f, 0, sizeof (f));
        sol = 0;
         
        gets (s + 1);
        n = strlen (s + 1);
         
        for (i = 1; i <= n; i ++)
            f[s[i] - 'a'] ++;
        for (i = 0; i <= 26; i ++)
            f[30] += f[i] % 2;
        if (f[30] > 1)
        {
            printf ("-1\n");
            continue;
        }
         
        st = 1;
        dr = n;
        while (st < dr)
        {
            if (s[st] == s[dr])
            {
                st ++; dr --;
                continue;
            }
            if (f[s[st] - 'a'] & 1)
            {
                for (i = st + 1; i < dr; i ++)
                    if (s[i] == s[dr])
                    {
                        for (; i > st; i --, sol ++)
                            s[i] = s[i - 1];
                        s[st] = s[dr];
                        break;
                    }
            }
            else
                for (i = dr - 1; i > st; i --)
                    if (s[i] == s[st])
                    {
                        for (; i < dr; i ++, sol ++)
                            s[i] = s[i + 1];
                        s[dr] = s[st];
                        break;
                    }
            st ++;
            dr --;
        }
        printf ("%d\n", sol);
    }
    return 0;
}