#include<iostream>
#include<fstream>
using namespace std;
 
ifstream f("note.in");
ofstream g("note.out");
 
#define MaxN 1100
#define MaxLim 110
 
int V,N,M;
int A[MaxN][MaxLim],B[MaxN];
int Best[MaxN][MaxN];
 
void citire(void)
{
    int a;
 
    f >> V >> N;
    for(int i=1;i<=V;i++)
        for(int j=1;j<=N;j++)
        {
            f >> a;
            A[j][a] = 1;
        }
 
    f >> M;
    for(int i=1;i<=M;i++)
        f >> B[i];
}
 
void dinamica(void)
{
    for(int i=1;i<=N || i<=M;i++)
        Best[i][0] = Best[0][i] = i;
 
    for(int i=1;i<=M;i++)
        for(int j=1;j<=N;j++)
        {
            Best[i][j] = Best[i-1][j-1] + (1-A[j][B[i]]);
            Best[i][j] = min(Best[i][j],min(Best[i-1][j],Best[i][j-1])+1);
        }
}
 
int main()
{
    citire();
    dinamica();
 
/*    for(int i=0;i<=M;i++)
    {
        for(int j=0;j<=N;j++)
            cout << Best[i][j] << " ";
        cout << "\n";
    }*/
 
    g << Best[M][N] << "\n";
}