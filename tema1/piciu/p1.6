#include<cstdio>
#include<cstring>
int t,i,n,nr[26];
char c[200],*a,aux;
 
inline void invert()
{
    for(i=0;i<n/2;i++){
        aux=a[i];
        a[i]=a[n-i-1];
        a[n-i-1]=aux;}
}
 
int make_pal()
{
    for(i=n-1;i>=0;i--)
        if(a[i]==a[0]) break;
    int nr=n-i-1;
    for(;i<n-1;i++)
        a[i]=a[i+1];
    return nr;
}
 
 
int cost_pal()
{
    n=strlen(a);
    if(n<=1) return 0;
    if(nr[a[0]-'a']==1) invert();
    int cost=make_pal();
    a[n-1]=0;
    nr[a[0]-'a']-=2;
    a++;
    return cost+cost_pal();
}
 
int main()
{
    freopen("palind.in","r",stdin);
    freopen("palind.out","w",stdout);
    scanf("%d",&t);
    for(;t;t--){
        memset(nr,0,sizeof(nr));
        scanf(" %s ",c);
        int n=strlen(c);
        for(i=0;i<n;i++)
            nr[c[i]-'a']++;
        n=n%2;
        n=-n;
        for(i=0;i<26;i++)
            n=n+nr[i]%2;
        if(n) {printf("-1\n");continue;}
        a=c;
        printf("%d\n",cost_pal());}
    fclose(stdout);
    return 0;
}