#include <algorithm>
#include <cstdio>
#include <cstring>
 
using namespace std;
 
const int N=1030;
 
int a[N][103], b[N], dp[N][N];
 
int main()
{
    freopen("note.in", "r", stdin);
    freopen("note.out", "w", stdout);
    int m, n, i, j, x;
    scanf("%d%d", &m, &n);
    for(i=1;i<=m;i++)
    {
        for(j=1;j<=n;j++)
        {
            scanf("%d", &x);
            if(x<101&&x>=0) a[j][x]=1;
        }
    }
    scanf("%d", &m);
    for(i=1;i<=m;i++) scanf("%d", &b[i]);
    memset(dp, 0x3f3f3f3f, sizeof(dp));
    dp[0][0]=0;
    dp[0][1]=1;
    dp[1][0]=1;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=m;j++)
        {
            if(dp[i-1][j]<dp[i][j-1]) dp[i][j]=dp[i-1][j]+1;
            else dp[i][j]=dp[i][j-1]+1;
            if(a[i][b[j]])
            {
                if(dp[i-1][j-1]<dp[i][j]) dp[i][j]=dp[i-1][j-1];
            }
            else if(dp[i-1][j-1]+1<dp[i][j]) dp[i][j]=dp[i-1][j-1]+1;
        }
    }
    printf("%d", dp[n][m]);
}