#include <cstdio>
 
int N, M, V, viz[125][1025], c[1025][1025], p[1025];
 
int min(int x, int y) { return x < y ? x : y;}
 
int main()
{
    freopen("note.in","r",stdin);
    freopen("note.out","w",stdout);
 
    int i, j, x;
 
    scanf("%d %d",&V,&N);
 
    for (i = 1; i <= V; i++)
        for (j = 1; j <= N; j++)
        {
            scanf("%d",&x);
            viz[x][j] = 1;
        }
 
    scanf("%d",&M);
    for (i = 1; i <= M; i++) scanf("%d",&p[i]);
 
    for(i = 1; i <= N; i++) c[0][i] = i;  
    for(i = 1; i <= M; i++) c[i][0] = i;  
 
    for (i = 1; i <= M; i++)
        for (j = 1; j <= N; j++)
        {
            if (viz[p[i]][j]) c[i][j] = c[i - 1][j - 1];
            else c[i][j] = 1 + min(c[i - 1][j - 1], min(c[i - 1][j], c[i][j - 1]));
        }
    printf("%d\n",c[M][N]);
    return 0;
}